{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SvnHub": {
    "DataFilePath": "data/SvnHub.json",
    "RepositoriesRootPath": "/srv/svn/repos",
    "AuthzPath": "/etc/svn/authz",
    "HtpasswdPath": "/etc/svn/htpasswd",
    "HtpasswdCommand": "htpasswd",
    "SvnadminCommand": "svnadmin",
    "SvnCommand": "svn",
    "SvnmuccCommand": "svnmucc",
    "SvnlookCommand": "svnlook",
    "ApacheReloadProgram": "systemctl",
    "ApacheReloadArguments": "reload apache2"
  },
  "AllowedHosts": "*"
}
